<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 1.2//EN"
"http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
 
<web-app>

    <display-name>OnlineQuizApp</display-name>
	
	<context-param>
		<param-name>welcome</param-name>
		<param-value>Lets start the quiz</param-value>
	</context-param>
	
	<context-param>
		<param-name>result</param-name>
		<param-value>Your final score as follows</param-value>
	</context-param>
	
    <servlet>
        <servlet-name>Quiz</servlet-name>
        <servlet-class>Quiz</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Quiz</servlet-name>
        <url-pattern>/quiz</url-pattern>
    </servlet-mapping>

	<welcome-file-list>  
		<welcome-file>index.htm</welcome-file>  
	</welcome-file-list>  

</web-app>



